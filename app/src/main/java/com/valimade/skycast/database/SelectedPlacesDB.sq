CREATE TABLE selectedPlacesDB (
  id INTEGER PRIMARY KEY NOT NULL,
  name TEXT NOT NULL,
  latitude REAL NOT NULL,
  longitude REAL NOT NULL
);

selectAll:
SELECT * FROM selectedPlacesDB;

insert:
INSERT INTO selectedPlacesDB(id, name, latitude, longitude)
VALUES (?, ?, ?, ?);

deleteById:
DELETE FROM selectedPlacesDB
WHERE id = ?;